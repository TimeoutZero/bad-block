{
  "manifest_version": 2,

  "name": "Bad block",
  "content_security_policy": " script-src 'self' 'unsafe-eval' http://localhost; object-src 'self'",
  "description": "__MSG_appDescription__",
  "version": "0.0.1",
  "default_locale" : "en",
  "incognito" : "not_allowed",

  "icons": {
    "19": "./images/logo19.png",
    "32": "./images/logo32.png",
    "128": "./images/logo128.png"
  },

  "page_action": {
    "default_icon": "./images/logo19.png",
    "default_popup": "./popupApp/index.html",
    "default_title": "Reactflix"
  },

  "content_scripts": [
    {
      "matches": [
        "*://www.g1.globo.com"
      ],
      "js": ["./features/content/feed.content.js"]
    }
  ],

  "background": {
    "scripts": []
  },

  "externally_connectable": {
    "matches": []
  },

  "web_accessible_resources" : [
    "assets/*",
    "images/*"
  ],

  "permissions": [
    "activeTab",
    "storage",
    "declarativeContent"
  ],

  "optional_permissions": [
    "*://www.g1.globo.com*"
  ]
}